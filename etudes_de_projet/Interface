@startuml
package Interface <<Folder>>{
class QWidget{}

class Pile {
    -ui:Pile*
    -size=5:size_t 
    +setMessage(m="":const QString )
    +updateContent(content:const list<QString> )
    +updateSize(t:size_t )
}

class Commandline{
    -ui:Commandline*
    -textContent: QString 
    -clock: QTimer*
    -cursor: bool 
    +addText(str:const QString )
    +clearText()
    +backspace();
    +updateText();
    +getText() const : QString ;
    }
    
    
class KeyboardFunctions{
   - ui:KeyboardFunctions*
   -dock:QDockWidget*
   -signalMapper :QDockWidget*
    +getDock() const :QDockWidget* 
    }
    

class KeyboardNumeric{
   - ui:KeyboardNumeric*
   -dock:QDockWidget*
   -signalMapper :QDockWidget*
    +getDock() const :QDockWidget* 
    }
    
    
class Programmes{
  -ui:Programmes*
  -dock:QDockWidget* 
  +getDock() const :QDockWidget*
  }
  
  
class Variables{ 
  -ui:Programmes*
  -dock:QDockWidget* 
  +getDock() const :QDockWidget*
  }

class Mainwindow{
    -ui:Mainwindow*
    -commandline:Commandline* 
    -pile:Pile* 
    -keyboardnumeric:KeyboardNumeric* 
    -keyboardFunctions:KeyboardFunctions* 
    -programmes:Programmes* 
    -variables:Variables* 
    +getContentCommandLine() const:const QString
    +setMessage(m:const QString)
    }
    
class QMainWindow{}


QWidget <|-- Programmes
QWidget <|-- KeyboardNumeric
QWidget <|-- KeyboardFunctions
QWidget <|-- Pile
