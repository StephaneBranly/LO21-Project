  + {virtual} executeOpe(e : vector<Expression*>)
  + eval()
  + {virtual} getCopy() const : Expression*
}
Expression <|-down- Operator

class OperatorAritBinary{
  - opes : map<tuple<string, string>, ActionBinary*>
